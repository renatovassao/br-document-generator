{
  "name": "br-document-generator",
  "version": "1.0.0",
  "description": "Brazilian Document Generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Renato Vass√£o",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "brazilian",
    "br",
    "document",
    "documento",
    "generator",
    "gerador"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.12",
    "@types/jest": "^24.0.17",
    "@types/request": "^2.48.2",
    "@types/request-promise": "^4.1.44",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renatovassao/br-document-generator.git"
  },
  "bugs": {
    "url": "https://github.com/renatovassao/br-document-generator/issues"
  },
  "homepage": "https://github.com/renatovassao/br-document-generator#readme"
}
